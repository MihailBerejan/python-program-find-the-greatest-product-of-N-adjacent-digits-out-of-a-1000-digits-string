'''
Find the n adjacent digits in the 1000-digit number that have the greatest product. What is the value of this product?
'''

my_string = ""

maxp = int(input("How many adjacent digits do you want to multiply: "))

my_lst = []
for i in range(len(my_string)):
    if len(my_string[i:i+maxp]) == maxp:
        my_lst = my_lst + [my_string[i:i+maxp]]
print("The list of %.0i digits strings resulted from the original 1000 digits string is: " %maxp)        
print(my_lst)

def prod(my_string, x):
    '''
    The prod function calculates the product of all x digits in a given string.
    '''
    if x == 0:
        return int(my_string[0])
    return int(my_string[x])*prod(my_string, x-1)

my_lst_2 = []
for j in range(len(my_lst)):
    my_lst_2 = my_lst_2 + [(prod(my_lst[j], maxp-1))]
    
print("\nThe list of products of %.0i digits is: " %maxp)
print(my_lst_2)
print("\nThe greatest product of %.0i adjacent digits is:" %maxp)
print(f"{max(my_lst_2):,}")

print("\nThe %.0i adjacent digits in the 1000-digit string that have this product are:" %maxp)
print(my_lst[my_lst_2.index(max(my_lst_2))])

del(my_lst)
del(my_lst_2)

